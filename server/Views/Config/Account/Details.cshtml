@using InfoScreenPi.Entities
@model User


<dl class="dl-horizontal">
    <dt>Gebruikersnaam</dt>
    <dd>@Model.Username</dd>

    <dt>Voornaam</dt>
    <dd>@Model.FirstName</dd>

    <dt>Achternaam</dt>
    <dd>@Model.LastName</dd>

    <dt>Email</dt>
    <dd>@Model.Email</dd>

    <dt>Laatste Login</dt>
    <dd>@Model.LastLogin</dd>
</dl>